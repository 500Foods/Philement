{
    "ServerName": "hydrogen-test-27-db2",
    "PayloadKey": "${env.PAYLOAD_KEY}",
    "WebServer": {
        "Enabled": false,
        "EnableIPv6": false,
        "Port": 5273
    },
    "Databases": {
        "ConnectionCount": 1,
        "Connections": [
            {
                "Enabled": true,
                "Name": "HTST",
                "Engine": "${env.HYDROTST_DB_TYPE}",
                "Database": "${env.HYDROTST_DB_NAME}",
                "Host": "localhost",
                "Port": "50000",
                "User": "${env.HYDROTST_DB_USER}",
                "Pass": "${env.HYDROTST_DB_PASS}",
                "Bootstrap":"SELECT * FROM acuranzo.queries;",
                "Queues": {
                    "Slow": {
                        "start": 1,
                        "min": 1,
                        "max": 4,
                        "up": 10,
                        "down": 2,
                        "inactivity": 300
                    },
                    "Medium": {
                        "start": 2,
                        "min": 1,
                        "max": 8,
                        "up": 15,
                        "down": 3,
                        "inactivity": 240
                    },
                    "Fast": {
                        "start": 4,
                        "min": 2,
                        "max": 16,
                        "up": 20,
                        "down": 5,
                        "inactivity": 180
                    },
                    "Cache": {
                        "start": 1,
                        "min": 1,
                        "max": 4,
                        "up": 5,
                        "down": 1,
                        "inactivity": 600
                    }
                }
            }
        ]
    }
}
