{
    "ServerName": "hydrogen-test-26",
    "PayloadKey": "${env.PAYLOAD_KEY}",
    "WebServer": {
        "Enabled": true,
        "EnableIPv6": false,
        "Port": 5221,
        "Headers": [
            ["*", "Cross-Origin-Opener-Policy", "same-origin"],
            ["*", "Cross-Origin-Embedder-Policy", "require-corp"],
            [".wasm", "Cross-Origin-Resource-Policy", "same-origin"],
            [".js", "Cross-Origin-Resource-Policy", "same-origin"]
          ]
    },
    "API": {
        "Enabled": true,
        "Prefix": "/api"
    },
    "Swagger": {
        "Enabled": true,
        "Prefix": "/apidocs",
        "UIOptions": {
            "TryItEnabled": true,
            "AlwaysExpanded": true,
            "DisplayOperationId": true,
            "DefaultModelsExpandDepth": 1,
            "DefaultModelExpandDepth": 1,
            "ShowExtensions": false,
            "ShowCommonExtensions": true,
            "DocExpansion": "list",
            "SyntaxHighlightTheme": "agate"
        }
    }
}