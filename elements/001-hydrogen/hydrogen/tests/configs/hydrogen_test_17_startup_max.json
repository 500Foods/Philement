{
    "Server": {
        "ServerName": "hydrogen-test-17",
        "PayloadKey": "${env.PAYLOAD_KEY}",
        "LogFile": "./logs/hydrogen_test_max.log"
    },
    "WebServer": {
        "EnableIPv4": true,
        "EnableIPv6": true,
        "Port": 5170,
        "WebRoot": "./www",
        "UploadPath": "/api/upload",
        "UploadDir": "/tmp/hydrogen_test_uploads",
        "MaxUploadSize": 2147483648
    },
    "WebSocketServer": {
        "EnableIPv4": true,
        "EnableIPv6": true,
        "LibLogLevel": 2,
        "Port": 5171,
        "Protocol": "hydrogen",
        "Key": "${env.WEBSOCKET_KEY}",
        "MaxMessageSize": 2048,
        "ConnectionTimeouts": {
            "ShutdownWaitSeconds": 2,
            "ServiceLoopDelayMs": 50,
            "ConnectionCleanupMs": 500,
            "ExitWaitSeconds": 3
        }
    },
    "Network": {
        "Available": {
            "lo": true,
            "wg0": true,     
            "vnet0": true,   
            "vnet1": false,
            "vnet2": false,
            "vnet3": false,
            "vnet4": false
        },
        "Interfaces": {
            "MaxInterfaces": 16,
            "MaxIPsPerInterface": 8,
            "MaxInterfaceNameLength": 16,
            "MaxIPAddressLength": 46
        },
        "PortAllocation": {
            "StartPort": 5000,
            "EndPort": 65535,
            "ReservedPorts": [22, 80, 443],
            "RetryAttempts": 5
        }
    },
    "SystemResources": {
        "Queues": {
            "MaxQueueBlocks": 128,
            "QueueHashSize": 256,
            "DefaultQueueCapacity": 1024
        },
        "Buffers": {
            "DefaultMessageBuffer": 1024,
            "MaxLogMessageSize": 2048,
            "PathBufferSize": 256,
            "LineBufferSize": 512
        }
    },
    "SystemMonitoring": {
        "Intervals": {
            "StatusUpdateMs": 1000,
            "ResourceCheckMs": 5000,
            "MetricsUpdateMs": 2000
        },
        "Thresholds": {
            "MemoryWarningPercent": 90,
            "DiskSpaceWarningPercent": 85,
            "LoadAverageWarning": 5.0
        }
    },
    "PrintQueue": {
        "Enabled": true,
        "QueueSettings": {
            "DefaultPriority": 1,
            "EmergencyPriority": 0,
            "MaintenancePriority": 2,
            "SystemPriority": 3
        },
        "Timeouts": {
            "ShutdownWaitMs": 500,
            "JobProcessingTimeoutMs": 1000
        },
        "Buffers": {
            "JobMessageSize": 256,
            "StatusMessageSize": 256
        }
    },
    "Logging": {
        "Console": {
            "Enabled": true,
            "DefaultLevel": 0,
            "Subsystems": {
                "ThreadMgmt": 0,
                "Shutdown": 0,
                "mDNSServer": 0,
                "WebServer": 0,
                "WebSocket": 0,
                "PrintQueue": 0,
                "LogQueueManager": 0,
                "Main": 0,
                "QueueSystem": 0,
                "SystemService": 0,
                "Utils": 0,
                "MemoryMetrics": 0,
                "Beryllium": 0,
                "Configuration": 0,
                "API": 0,
                "Initialization": 0,
                "Network": 0
            }
        },
        "Database": {
            "Enabled": true,
            "DefaultLevel": 0,
            "ConnectionString": "sqlite://./hydrogen_test_max.db",
            "Subsystems": {
                "ThreadMgmt": 0,
                "Shutdown": 0,
                "mDNSServer": 0,
                "WebServer": 0,
                "WebSocket": 0,
                "PrintQueue": 0,
                "LogQueueManager": 0,
                "Main": 0,
                "QueueSystem": 0,
                "SystemService": 0,
                "Utils": 0,
                "MemoryMetrics": 0,
                "Beryllium": 0,
                "Configuration": 0,
                "API": 0,
                "Initialization": 0,
                "Network": 0
            }
        },
        "File": {
            "Enabled": true,
            "DefaultLevel": 0,
            "Path": "./hydrogen_test_max.log",
            "Subsystems": {
                "ThreadMgmt": 0,
                "Shutdown": 0,
                "mDNSServer": 0,
                "WebServer": 0,
                "WebSocket": 0,
                "PrintQueue": 0,
                "LogQueueManager": 0,
                "Main": 0,
                "QueueSystem": 0,
                "SystemService": 0,
                "Utils": 0,
                "MemoryMetrics": 0,
                "Beryllium": 0,
                "Configuration": 0,
                "API": 0,
                "Initialization": 0,
                "Network": 0
            }
        }
    },
    "Swagger": {
        "Enabled": true,
        "Prefix": "/swagger",
        "UIOptions": {
            "TryItEnabled": true,
            "AlwaysExpanded": true,
            "DisplayOperationId": true,
            "DefaultModelsExpandDepth": 1,
            "DefaultModelExpandDepth": 1,
            "ShowExtensions": false,
            "ShowCommonExtensions": true,
            "DocExpansion": "list",
            "SyntaxHighlightTheme": "agate"
        }
    },
    "Terminal": {
        "Enabled": true,
        "WebPath": "/terminal",
        "ShellCommand": "/bin/bash",
        "MaxSessions": 4,
        "IdleTimeoutSeconds": 600,
        "BufferSize": 4096
    }
}