{
    "Server": {
        "ServerName": "hydrogen-test-26",
        "PayloadKey": "${env.PAYLOAD_KEY}"
    },
    "WebServer": {
        "EnableIPv4": true,
        "EnableIPv6": false,
        "Port": 5220,
        "Headers": [
            ["*", "X-Custom-Header-1", "test-value-1"],
            [".js", "X-JS-Specific", "javascript-file"]
          ]
    },
    "API": {
        "Enabled": true,
        "Prefix": "/api"
    },
    "Swagger": {
        "Enabled": true,
        "Prefix": "/swagger",
        "UIOptions": {
            "TryItEnabled": true,
            "AlwaysExpanded": true,
            "DisplayOperationId": true,
            "DefaultModelsExpandDepth": 1,
            "DefaultModelExpandDepth": 1,
            "ShowExtensions": false,
            "ShowCommonExtensions": true,
            "DocExpansion": "list",
            "SyntaxHighlightTheme": "agate"
        }
    }
}