{
    "ServerName": "Philement/hydrogen-test-swagger-test-1",
    "LogFile": "./logs/hydrogen_test_swagger_test_1.log",
    "WebServer": {
        "Enabled": true,
        "EnableIPv6": true,
        "Port": 5034,
        "WebRoot": "./www",
        "UploadPath": "/api/upload",
        "UploadDir": "/tmp/hydrogen_test_uploads",
        "MaxUploadSize": 2147483648
    },
    "API": {
        "Enabled": true,
        "Prefix": "/api",
        "JWTSecret": "test-jwt-secret-for-swagger-test-1",
        "TokenExpireMinutes": 60,
        "RefreshTokenExpireMinutes": 1440,
        "Metadata": {
            "Title": "Hydrogen API",
            "Description": "Hydrogen Server API",
            "Version": "1.0.0"
        },
        "Security": {
            "Enabled": true,
            "TokenSecret": "test-jwt-secret-for-swagger-test-1",
            "TokenExpireMinutes": 60
        }
    },    "WebSocketServer": {
        "EnableIPv4": false,
        "EnableIPv6": false,
        "LibLogLevel": 2,
        "Port": 5001,
        "Protocol": "hydrogen",
        "Key": "${env.WEBSOCKET_KEY}",
        "MaxMessageSize": 2048,
        "ConnectionTimeouts": {
            "ShutdownWaitSeconds": 2,
            "ServiceLoopDelayMs": 50,
            "ConnectionCleanupMs": 500,
            "ExitWaitSeconds": 3
        }
    },
    "Network": {
        "Interfaces": {
            "MaxInterfaces": 16,
            "MaxIPsPerInterface": 8,
            "MaxInterfaceNameLength": 16,
            "MaxIPAddressLength": 46
        },
        "PortAllocation": {
            "StartPort": 5040,
            "EndPort": 65535,
            "ReservedPorts": [22, 80, 443],
            "RetryAttempts": 5
        }
    },
    "SystemResources": {
        "Queues": {
            "MaxQueueBlocks": 128,
            "QueueHashSize": 256,
            "DefaultQueueCapacity": 1024
        },
        "Buffers": {
            "DefaultMessageBuffer": 1024,
            "MaxLogMessageSize": 2048,
            "PathBufferSize": 256,
            "LineBufferSize": 512
        }
    },
    "SystemMonitoring": {
        "Intervals": {
            "StatusUpdateMs": 1000,
            "ResourceCheckMs": 5000,
            "MetricsUpdateMs": 2000
        },
        "Thresholds": {
            "MemoryWarningPercent": 90,
            "DiskSpaceWarningPercent": 85,
            "LoadAverageWarning": 5.0
        }
    },
    "PrintQueue": {
        "Enabled": true,
        "QueueSettings": {
            "DefaultPriority": 1,
            "EmergencyPriority": 0,
            "MaintenancePriority": 2,
            "SystemPriority": 3
        },
        "Timeouts": {
            "ShutdownWaitMs": 500,
            "JobProcessingTimeoutMs": 1000
        },
        "Buffers": {
            "JobMessageSize": 256,
            "StatusMessageSize": 256
        }
    },
    "Logging": {
        "Levels": [
            [0, "ALL"],
            [1, "INFO"],
            [2, "WARN"],
            [3, "DEBUG"],
            [4, "ERROR"],
            [5, "CRITICAL"],
            [6, "NONE"]
        ],
        "Console": {
            "Enabled": true,
            "DefaultLevel": 0,
            "Subsystems": {
                "ThreadMgmt": 0,
                "Shutdown": 0,
                "mDNSServer": 0,
                "WebServer": 0,
                "WebSocket": 0,
                "PrintQueue": 0,
                "LogQueueManager": 0,
                "Main": 0,
                "QueueSystem": 0,
                "SystemService": 0,
                "Utils": 0,
                "MemoryMetrics": 0,
                "Beryllium": 0,
                "Configuration": 0,
                "API": 0,
                "Initialization": 0,
                "Network": 0
            }
        },
        "Database": {
            "Enabled": true,
            "DefaultLevel": 0,
            "ConnectionString": "sqlite://./tests/hydrogen_test_swagger_test_1.db",
            "Subsystems": {
                "ThreadMgmt": 0,
                "Shutdown": 0,
                "mDNSServer": 0,
                "WebServer": 0,
                "WebSocket": 0,
                "PrintQueue": 0,
                "LogQueueManager": 0,
                "Main": 0,
                "QueueSystem": 0,
                "SystemService": 0,
                "Utils": 0,
                "MemoryMetrics": 0,
                "Beryllium": 0,
                "Configuration": 0,
                "API": 0,
                "Initialization": 0,
                "Network": 0
            }
        },
        "File": {
            "Enabled": true,
            "DefaultLevel": 0,
            "Path": "./tests/hydrogen_test_swagger_test_1.log",
            "Subsystems": {
                "ThreadMgmt": 0,
                "Shutdown": 0,
                "mDNSServer": 0,
                "WebServer": 0,
                "WebSocket": 0,
                "PrintQueue": 0,
                "LogQueueManager": 0,
                "Main": 0,
                "QueueSystem": 0,
                "SystemService": 0,
                "Utils": 0,
                "MemoryMetrics": 0,
                "Beryllium": 0,
                "Configuration": 0,
                "API": 0,
                "Initialization": 0,
                "Network": 0
            }
        }
    },
    "Swagger": {
        "Enabled": true,
        "Prefix": "/swagger",
        "UIOptions": {
            "TryItEnabled": true,
            "AlwaysExpanded": true,
            "DisplayOperationId": true,
            "DefaultModelsExpandDepth": 1,
            "DefaultModelExpandDepth": 1,
            "ShowExtensions": false,
            "ShowCommonExtensions": true,
            "DocExpansion": "list",
            "SyntaxHighlightTheme": "agate"
        }
    },
    "Terminal": {
        "Enabled": true,
        "WebPath": "/terminal",
        "ShellCommand": "/bin/bash",
        "MaxSessions": 4,
        "IdleTimeoutSeconds": 600,
        "BufferSize": 4096
    },
    "Databases": {
        "Workers": 1,
        "Connections": {
            "OIDC": {
                "Type": "postgres",
                "Host": "{$env.TEST_OIDC_DB_HOST}",
                "Port": "{$env.TEST_OIDC_DB_PORT}",
                "Database": "{$env.TEST_OIDC_DB_NAME}",
                "Username": "{$env.TEST_OIDC_DB_USER}",
                "Password": "{$env.TEST_OIDC_DB_PASS}",
                "MaxConnections": 10
            },
            "Acuranzo": {
                "Type": "postgres",
                "Host": "{$env.TEST_ACURANZO_DB_HOST}",
                "Port": "{$env.TEST_ACURANZO_DB_PORT}",
                "Database": "{$env.TEST_ACURANZO_DB_NAME}",
                "Username": "{$env.TEST_ACURANZO_DB_USER}",
                "Password": "{$env.TEST_ACURANZO_DB_PASS}",
                "MaxConnections": 10
            },
            "Helium": {
                "Type": "postgres",
                "Host": "{$env.TEST_HELIUM_DB_HOST}",
                "Port": "{$env.TEST_HELIUM_DB_PORT}",
                "Database": "{$env.TEST_HELIUM_DB_NAME}",
                "Username": "{$env.TEST_HELIUM_DB_USER}",
                "Password": "{$env.TEST_HELIUM_DB_PASS}",
                "MaxConnections": 10
            },
            "Canvas": {
                "Type": "postgres",
                "Host": "{$env.TEST_CANVAS_DB_HOST}",
                "Port": "{$env.TEST_CANVAS_DB_PORT}",
                "Database": "{$env.TEST_CANVAS_DB_NAME}",
                "Username": "{$env.TEST_CANVAS_DB_USER}",
                "Password": "{$env.TEST_CANVAS_DB_PASS}",
                "MaxConnections": 10
            }
        }
    },
    "SMTPRelay": {
        "Enabled": true,
        "ListenPort": 587,
        "Workers": 2,
        "QueueSettings": {
            "MaxQueueSize": 1000,
            "RetryAttempts": 3,
            "RetryDelaySeconds": 300
        },
        "OutboundServers": [
            {
                "Host": "{$env.TEST_SMTP_SERVER1_HOST}",
                "Port": "{$env.TEST_SMTP_SERVER1_PORT}",
                "Username": "{$env.TEST_SMTP_SERVER1_USER}",
                "Password": "{$env.TEST_SMTP_SERVER1_PASS}",
                "UseTLS": true
            },
            {
                "Host": "{$env.TEST_SMTP_SERVER2_HOST}",
                "Port": "{$env.TEST_SMTP_SERVER2_PORT}",
                "Username": "{$env.TEST_SMTP_SERVER2_USER}",
                "Password": "{$env.TEST_SMTP_SERVER2_PASS}",
                "UseTLS": true
            }
        ]
    },
    "mDNSClient": {
        "Enabled": true,
        "EnableIPv6": true,
        "ScanIntervalMs": 5000,
        "ServiceTypes": [
            "_http._tcp",
            "_octoprint._tcp",
            "_websocket._tcp",
            "_hydrogen._tcp"
        ],
        "MonitoredServices": {
            "OwnServices": true,
            "LoadBalancers": true,
            "PrinterServices": true,
            "CustomServices": ["_test._tcp"]
        },
        "HealthCheck": {
            "Enabled": true,
            "IntervalMs": 30000,
            "TimeoutMs": 5000,
            "RetryCount": 3
        }
    },
    "mDNSServer": {
        "Enabled": true,
        "EnableIPv6": true,
        "DeviceId": "hydrogen-test-swagger-test-1",
        "FriendlyName": "Hydrogen Test Swagger Test 1",
        "Model": "Hydrogen-Test",
        "Manufacturer": "Philement",
        "Version": "0.1.0",
        "Services": [
            {
                "Name": "Hydrogen_Test_Web_Interface",
                "Type": "_http._tcp",
                "Port": 5034,
                "TxtRecords": "path=/api/upload"
            },
            {
                "Name": "Hydrogen_Test_OctoPrint_Emulation",
                "Type": "_octoprint._tcp",
                "Port": 5034,
                "TxtRecords": "path=/api,version=1.1.0"
            },
            {
                "Name": "Hydrogen_Test_WebSocket",
                "Type": "_websocket._tcp",
                "Port": 5035,
                "TxtRecords": "path=/websocket"
            }
        ]
    }
}