{
    "ServerName": "${env.H_SERVER_NAME}",
    "PayloadKey": "${env.PAYLOAD_KEY}",
    "LogFile": "./logs/hydrogen_env_test.log",
    "WebServer": {
        "EnableIPv4": "${env.H_WEB_ENABLED}",
        "EnableIPv6": false,
        "Port": "${env.H_WEB_PORT}",
        "WebRoot": "./www",
        "UploadPath": "/api/upload",
        "UploadDir": "${env.H_UPLOAD_DIR}",
        "MaxUploadSize": "${env.H_MAX_UPLOAD_SIZE}"
    },
    "WebSocketServer": {
        "EnableIPv4": false,
        "EnableIPv6": false,
        "LibLogLevel": 2,
        "Port": 5001,
        "Protocol": "hydrogen",
        "Key": "${env.WEBSOCKET_KEY}",
        "MaxMessageSize": 2048,
        "ConnectionTimeouts": {
            "ShutdownWaitSeconds": 2,
            "ServiceLoopDelayMs": 50,
            "ConnectionCleanupMs": 500,
            "ExitWaitSeconds": 3
        }
    },
    "Network": {
        "Interfaces": {
            "MaxInterfaces": 16,
            "MaxIPsPerInterface": 8,
            "MaxInterfaceNameLength": 16,
            "MaxIPAddressLength": 46
        },
        "PortAllocation": {
            "StartPort": 5000,
            "EndPort": 65535,
            "ReservedPorts": [22, 80, 443],
            "RetryAttempts": 5
        }
    },
    "SystemResources": {
        "Queues": {
            "MaxQueueBlocks": "${env.H_MAX_QUEUE_BLOCKS}",
            "QueueHashSize": 256,
            "DefaultQueueCapacity": "${env.H_DEFAULT_QUEUE_CAPACITY}"
        },
        "Buffers": {
            "DefaultMessageBuffer": 1024,
            "MaxLogMessageSize": 2048,
            "PathBufferSize": 256,
            "LineBufferSize": 512
        }
    },
    "SystemMonitoring": {
        "Intervals": {
            "StatusUpdateMs": 1000,
            "ResourceCheckMs": 5000,
            "MetricsUpdateMs": 2000
        },
        "Thresholds": {
            "MemoryWarningPercent": "${env.H_MEMORY_WARNING}",
            "DiskSpaceWarningPercent": 85,
            "LoadAverageWarning": "${env.H_LOAD_WARNING}"
        }
    },
    "PrintQueue": {
        "Enabled": "${env.H_PRINT_QUEUE_ENABLED}",
        "QueueSettings": {
            "DefaultPriority": 1,
            "EmergencyPriority": 0,
            "MaintenancePriority": 2,
            "SystemPriority": 3
        },
        "Timeouts": {
            "ShutdownWaitMs": 500,
            "JobProcessingTimeoutMs": 1000
        },
        "Buffers": {
            "JobMessageSize": 256,
            "StatusMessageSize": 256
        }
    },
    "Logging": {
        "Levels": [
            [0, "ALL"],
            [1, "INFO"],
            [2, "WARN"],
            [3, "DEBUG"],
            [4, "ERROR"],
            [5, "CRITICAL"],
            [6, "NONE"]
        ],
        "Console": {
            "Enabled": true,
            "DefaultLevel": "${env.H_CONSOLE_LOG_LEVEL}",
            "Subsystems": {
                "ThreadMgmt": 0,
                "Shutdown": 0,
                "mDNSServer": 0,
                "WebServer": 0,
                "WebSocket": 0,
                "PrintQueue": 0,
                "LogQueueManager": 0,
                "Main": 0,
                "QueueSystem": 0,
                "SystemService": 0,
                "Utils": 0,
                "MemoryMetrics": 0,
                "Beryllium": 0,
                "Configuration": 0,
                "Environment": 0,
                "API": 0,
                "Initialization": 0,
                "Network": 0
            }
        },
        "Database": {
            "Enabled": true,
            "DefaultLevel": 0,
            "ConnectionString": "sqlite://./hydrogen_test_env.db",
            "Subsystems": {
                "ThreadMgmt": 0,
                "Shutdown": 0,
                "mDNSServer": 0,
                "WebServer": 0,
                "WebSocket": 0,
                "PrintQueue": 0,
                "LogQueueManager": 0,
                "Main": 0,
                "QueueSystem": 0,
                "SystemService": 0,
                "Utils": 0,
                "MemoryMetrics": 0,
                "Beryllium": 0,
                "Configuration": 0,
                "Environment": 0,
                "API": 0,
                "Initialization": 0,
                "Network": 0
            }
        },
        "File": {
            "Enabled": true,
            "DefaultLevel": 0,
            "Path": "./hydrogen_env_test.log",
            "Subsystems": {
                "ThreadMgmt": 0,
                "Shutdown": 0,
                "mDNSServer": 0,
                "WebServer": 0,
                "WebSocket": 0,
                "PrintQueue": 0,
                "LogQueueManager": 0,
                "Main": 0,
                "QueueSystem": 0,
                "SystemService": 0,
                "Utils": 0,
                "MemoryMetrics": 0,
                "Beryllium": 0,
                "Configuration": 0,
                "Environment": 0,
                "API": 0,
                "Initialization": 0,
                "Network": 0
            }
        }
    },
    "mDNSServer": {
        "Enabled": false,
        "EnableIPv6": false,
        "DeviceId": "${env.H_DEVICE_ID}",
        "FriendlyName": "${env.H_FRIENDLY_NAME}",
        "Model": "Hydrogen-Test",
        "Manufacturer": "Philement",
        "Version": "0.1.0",
        "Services": []
    }
}