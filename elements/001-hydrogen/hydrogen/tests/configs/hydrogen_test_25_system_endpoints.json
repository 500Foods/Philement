{
    "ServerName": "Philement/hydrogen-test-system-endpoints",
    "PayloadKey": "${env.PAYLOAD_KEY}",
    "LogFile": "./logs/hydrogen_test_system_endpoints.log",
    "WebServer": {
        "Enabled": true,
        "EnableIPv6": false,
        "Port": 5250,
        "WebRoot": "/tmp/www",
        "UploadPath": "/api/upload",
        "UploadDir": "/tmp/hydrogen_test_uploads",
        "MaxUploadSize": 2147483648,
        "ThreadPoolSize": 64,
        "MaxConnections": 2000,
        "MaxConnectionsPerIP": 500,
        "ConnectionTimeout": 60
    },
    "API": {
        "Enabled": true,
        "Prefix": "/api",
        "JWTSecret": "test-jwt-secret-for-system-endpoints",
        "TokenExpireMinutes": 60,
        "RefreshTokenExpireMinutes": 1440,
        "Metadata": {
            "Title": "Hydrogen API",
            "Description": "Hydrogen Server API",
            "Version": "1.0.0"
        },
        "Security": {
            "Enabled": true,
            "TokenSecret": "test-jwt-secret-for-system-endpoints",
            "TokenExpireMinutes": 60
        }
    },
    "WebSocketServer": {
        "EnableIPv4": false,
        "EnableIPv6": false,
        "LibLogLevel": 2,
        "Port": 5001,
        "Protocol": "hydrogen",
        "Key": "${env.WEBSOCKET_KEY}",
        "MaxMessageSize": 2048,
        "ConnectionTimeouts": {
            "ShutdownWaitSeconds": 2,
            "ServiceLoopDelayMs": 50,
            "ConnectionCleanupMs": 500,
            "ExitWaitSeconds": 3
        }
    },
    "Network": {
        "Interfaces": {
            "MaxInterfaces": 16,
            "MaxIPsPerInterface": 8,
            "MaxInterfaceNameLength": 16,
            "MaxIPAddressLength": 46
        },
        "PortAllocation": {
            "StartPort": 5032,
            "EndPort": 65535,
            "ReservedPorts": [22, 80, 443],
            "RetryAttempts": 5
        },
        "Available": {
            "all": true
        }
    },
    "SystemResources": {
        "Queues": {
            "MaxQueueBlocks": 128,
            "QueueHashSize": 256,
            "DefaultQueueCapacity": 1024
        },
        "Buffers": {
            "DefaultMessageBuffer": 1024,
            "MaxLogMessageSize": 2048,
            "PathBufferSize": 256,
            "LineBufferSize": 512
        }
    },
    "SystemMonitoring": {
        "Intervals": {
            "StatusUpdateMs": 1000,
            "ResourceCheckMs": 5000,
            "MetricsUpdateMs": 2000
        },
        "Thresholds": {
            "MemoryWarningPercent": 90,
            "DiskSpaceWarningPercent": 85,
            "LoadAverageWarning": 5.0
        }
    },
    "PrintQueue": {
        "Enabled": false,
        "QueueSettings": {
            "DefaultPriority": 1,
            "EmergencyPriority": 0,
            "MaintenancePriority": 2,
            "SystemPriority": 3
        },
        "Timeouts": {
            "ShutdownWaitMs": 500,
            "JobProcessingTimeoutMs": 1000
        },
        "Buffers": {
            "JobMessageSize": 256,
            "StatusMessageSize": 256
        }
