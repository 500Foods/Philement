{       
    "Server": {
        "ServerName": "hydrogen-test-26-terminal-payload",
        "PayloadKey": "${env.PAYLOAD_KEY}"
    },
    "WebServer": {
        "EnableIPv4": true,
        "EnableIPv6": true,
        "Port": 5260
    },
    "WebSocketServer": {
        "EnableIPv4": true,
        "EnableIPv6": true,
        "Port": 5261,
        "Protocol": "terminal",
        "Key": "${env.WEBSOCKET_KEY}",
        "MaxMessageSize": 8192
    },
    "Terminal": {
        "Enabled": true,
        "ShellCommand": "/bin/zsh",
        "MaxSessions": 10,
        "IdleTimeoutSeconds": 300,
        "BufferSize": 1024
    },
    "API": {
        "Enabled": true,
        "Prefix": "/api"
    }
}
