{
  "title": "Hydrogen Build Metrics Auto Report",
  "description": "Automated metrics visualization for Hydrogen project",
  "version": "1.0.0",
  "mode": "auto",
  "dateRange": {
    "start": "30_days_ago",
    "end": "today",
    "mode": "relative"
  },
  "chartSettings": {
    "width": 1920,
    "height": 1080,
    "margin": {
      "top": 50,
      "right": 80,
      "bottom": 50,
      "left": 80
    },
    "animationDuration": 500,
    "transitionDuration": 200,
    "showGrid": true,
    "showLegend": true,
    "showTooltip": false,
    "responsive": false,
    "outputFormat": "svg",
    "outputFile": "hydrogen_metrics_report.svg"
  },
  "metrics": [

    {
      "path": "scloc.main.Core_C_Headers.code",
      "label": "Core code",
      "axis": "left",
      "type": "line",
      "color": "#000080",
      "lineWidth": 2,
      "lineStyle": "regular",
      "pointRadius": 4,
      "showPoints": true,
      "interpolation": "monotone"
    },
    {
      "path": "cloc.main.Test_C_Headers.code",
      "label": "Test code",
      "axis": "left",
      "type": "line",
      "color": "#800000",
      "lineWidth": 2,
      "lineStyle": "regular",
      "pointRadius": 4,
      "showPoints": true,
      "interpolation": "monotone"
    },
    {
      "path": "cloc.main.Shell.code",
      "label": "Shell code",
      "axis": "left",
      "type": "line",
      "color": "#008000",
      "lineWidth": 2,
      "lineStyle": "regular",
      "pointRadius": 4,
      "showPoints": true,
      "interpolation": "monotone"
    }
  ],
  "uiSettings": {
    "theme": "dark",
    "showControlPanel": false,
    "headlessMode": true,
    "autoExport": true,
    "verboseLogging": false
  },
  "dataSources": {
    "local": {
      "basePath": "/docs/H/metrics",
      "fallbackDays": 30,
      "cacheEnabled": false
    }
  },
  "exportSettings": {
    "svg": {
      "filename": "hydrogen_metrics_report",
      "includeStyles": true,
      "includeScripts": false,
      "quality": "high",
      "backgroundColor": "#000409",
      "embedFonts": false
    }
  },
  "performance": {
    "maxConcurrentRequests": 5,
    "requestTimeout": 10000,
    "retryAttempts": 3,
    "retryDelay": 1000
  }
}